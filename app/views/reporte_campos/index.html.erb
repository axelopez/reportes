<div class="box grid_12">
    
        <div class="box-head"><h2>Campos de Reportes</h2></div>
        <div class="box-content">
            <table>
  <thead>
    <tr>
      <th>Reporte</th>
      <th>Campo</th>
      <th>Descripcion</th>
      <th>Tipo</th>
      <th>Sumatoria</th>
      <th>Orden</th>
      <th>Visible</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reporte_campos.each do |reporte_campo| %>
      <tr>
        <td><%= reporte_campo.reporte_id %></td>
        <td><%= reporte_campo.campo %></td>
        <td><%= reporte_campo.descripcion %></td>
        <td><%= reporte_campo.tipo %></td>
        <td><%= reporte_campo.sumatoria %></td>
        <td><%= reporte_campo.orden %></td>
        <td><%= reporte_campo.visible %></td>
        <td><%= link_to 'Show', reporte_campo %></td>
        
        <td><%= link_to 'Edit', reporte_campo_path(reporte_campo) %></td>
        <td><%= link_to 'Destroy', reporte_campo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reporte campo', new_reporte_campo_path %>

            </div>

        </div>
    




